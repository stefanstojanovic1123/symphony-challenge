{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
	"@fortawesome/free-solid-svg-icons": "^5.15.2",
	"@fortawesome/react-fontawesome": "^0.1.14",
	"dayjs": "^1.10.4",
	"dompurify": "^2.2.6",
	"quill": "^1.3.7",
	"react-redux": "^7.2.2",
	"react-router-dom": "^5.2.0",
	"react-spring": "^8.0.27",
	"redux": "^4.0.5",
	"tailwindcss": "^2.0.3"
  },
	"devDependencies": {
		"@babel/core": "7.12.13",
		"@babel/eslint-parser": "7.12.13",
		"@babel/eslint-plugin": "7.12.13",
		"@babel/plugin-proposal-class-properties": "7.12.13",
		"@babel/plugin-transform-react-constant-elements": "7.12.13",
		"@babel/plugin-transform-react-inline-elements": "7.12.13",
		"@babel/plugin-transform-runtime": "7.12.15",
		"@babel/preset-env": "7.12.13",
		"@babel/preset-react": "7.12.13",
		"@babel/runtime": "7.12.13",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
		"autoprefixer": "^10.2.4",
		"babel-loader": "^8.2.2",
		"copy-webpack-plugin": "^6.4.1",
		"css-loader": "^5.0.2",
		"eslint": "^7.19.0",
		"eslint-plugin-react": "^7.22.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"file-loader": "^6.1.1",
		"mini-css-extract-plugin": "^1.3.6",
		"postcss": "^8.2.6",
		"postcss-import": "^14.0.0",
		"postcss-loader": "^4.2.0",
		"prettier": "^2.2.1",
		"react-refresh": "^0.9.0",
		"rimraf": "^3.0.2",
		"style-loader": "^2.0.0",
		"webpack": "^4.46.0",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.2",
		"webpack-merge": "4.2.2"
	},
  "scripts": {
		"clean": "rimraf dist",
		"build": "yarn run clean && webpack --config webpack.config.dist.prod.js --progress",
		"build-stage": "yarn run clean && webpack --config webpack.config.dist.stage.js --progress",
		"serve": "webpack-dev-server --config webpack.config.dev.prod.js --progress",
		"serve-stage": "webpack-dev-server --config webpack.config.dev.stage.js --progress",
		"serve-local": "webpack-dev-server --config webpack.config.dev.local.js --progress",
		"build-silent": "yarn run clean && webpack --config webpack.config.dist.prod.js",
		"build-silent-stage": "yarn run clean && webpack --config webpack.config.dist.stage.js",
		"lint": "eslint --cache src/",
		"format": "prettier --write 'src/**/*.js' 'src/**/*.css'",
		"format-check": "prettier --check 'src/**/*.js' 'src/**/*.css'",
		"stats": "yarn run clean && webpack --config webpack.config.dist.prod.js --json > stats.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
